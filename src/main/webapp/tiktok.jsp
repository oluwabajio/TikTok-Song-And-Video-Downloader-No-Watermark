<script>

    $('#btn_watermark').hide();
    $('#btn_no_watermark').hide();
    $('#btn_audio').hide();

    $('#btn_download').click(function (e) {
        $.post("TikTok",
            {
                url: $('#tiktokurl').val(),
                action: "watermark_video"
            },
            function (result) {
                // alert(result);
                $('#btn_watermark').show();
                $('#btn_watermark').attr("href", result);

            });
    });
    $('#btn_download').click(function (e) {
        $.post("TikTok",
            {
                url: $('#tiktokurl').val(),
                action: "no_watermark_video"
            },
            function (result) {
                // alert(result);
                $('#btn_no_watermark').show();
                $('#btn_no_watermark').attr("href", result);
            });
    });

    $('#btn_download').click(function (e) {
        $.post("TikTok",
            {
                url: $('#tiktokurl').val(),
                action: "download_audio"
            },
            function (result) {
              //  alert(result);
                $('#btn_audio').show();
                $('#btn_audio').attr("href", result)
            });
    });


</script>



